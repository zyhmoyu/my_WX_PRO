/* pages/dk/dk.wxss */

.dkg{
	background: #FFFFFF;
	padding: 30rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	position: fixed;
	width: 100%;
	bottom: 75rpx;
	z-index: 2;
}
.dkg text{
	display: inline-block;
	width: 500rpx;
	padding: 10rpx 0;
	border-radius: 10rpx;
	color: #FFFFFF;
	text-align: center;
	font-size: 14px;
}
.dk1 text{
	background: #EA1D10;
}
.dk2 text{
	background: #D9D9D9;
}
.tc{
	height: 100rpx;
	width: 100%;
	background: #FFFFFF;
	position: fixed;
	bottom: 0;
	z-index: 1;
}